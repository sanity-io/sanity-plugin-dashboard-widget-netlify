  "homepage": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "abort-controller": "^2.0.3",
