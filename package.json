{
  "name": "sanity-plugin-dashboard-widget-netlify",
  "version": "2.0.2",
  "description": "Sanity Studio Dashboard Widget for triggering Netlify builds",
  "keywords": [
    "sanity",
    "plugin",
    "dashboard",
    "widget",
    "netlify"
  ],
  "homepage": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/sanity-plugin-dashboard-widget-netlify.git"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "__test": "jest --coverage",
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "clean": "rimraf lib",
    "compile": "tsc --noEmit",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run compile && npm run build",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "test:watch": "jest --coverage --watch",
    "watch": "pkg-utils watch --strict"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^2.16.4",
    "abort-controller": "^2.0.3",
    "react-props-stream": "^1.0.1",
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/dashboard": "^4.1.4",
    "@sanity/pkg-utils": "^7.9.3",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "react": "^18",
    "react-dom": "^18",
    "rimraf": "^3.0.2",
    "sanity": "^3.0.0",
    "styled-components": "^6.1.19",
    "ts-jest": "^29.0.3",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "@sanity/dashboard": "^4.1.4",
    "react": "^18.3 || ^19",
    "react-dom": "^18.3 || ^19",
    "sanity": "^3 || ^4.0.0-0 || ^5.0.0",
    "styled-components": "^6.1"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "babelConfig": false
    }
  }
}
