{
  "name": "sanity-plugin-dashboard-widget-netlify",
  "version": "0.1.4",
  "description": "Sanity Studio Dashboard Widget for triggering Netlify builds",
  "keywords": [
    "sanity",
    "plugin",
    "dashboard",
    "widget",
    "netlify"
  ],
  "main": "dist/sanity-plugin-dashboard-widget-netlify.umd.js",
  "module": "dist/sanity-plugin-dashboard-widget-netlify.es5.js",
  "typings": "dist/types/sanity-plugin-dashboard-widget-netlify.d.ts",
  "files": [
    "dist",
    "sanity.json"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify/issues"
  },
  "homepage": "https://github.com/sanity-io/sanity-plugin-dashboard-widget-netlify#readme",
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run postcss:build && typedoc --out docs --target es6 --theme minimal --mode file src",
    "watch-tsc": "tsc --watch --module commonjs",
    "watch-css": "postcss src/**/*.css --base src --dir dist/lib --watch",
    "watch-rollup": "rollup -c rollup.config.ts -w",
    "postcss:build": "postcss src/**/*.css --base src --dir dist/lib",
    "watch": "rollup -c rollup.config.ts && run-p watch-*",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@sanity/base": ">= 0.135.0",
    "@sanity/components": ">= 0.135.0",
    "react": "^16.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@sanity/base": "^0.140.9",
    "@sanity/components": "^0.140.9",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "@types/react": "^16.8.7",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^24.3.1",
    "jest-config": "^24.3.1",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "react": "^16.8.4",
    "react-hot-loader": "^4.8.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-postcss-modules": "^2.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.19.3",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths-jest": "0.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "abort-controller": "^2.0.3",
    "react-props-stream": "^1.0.0"
  }
}
